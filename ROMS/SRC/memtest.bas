10 FOR W = 36864 TO 65024 STEP 256
20 FOR Q = 0 TO 255
30 POKE W + Q, Q
35 PRINT W + Q;" "; Q
40 NEXT Q
50 FOR R = 0 TO 255
60 LET E = PEEK (W + R)
65 PRINT W + R;" "; E
80 IF E <> R THEN GOSUB 1000
90 NEXT R
100 PRINT W
110 NEXT W
120 PRINT "Done."
130 STOP
1000 PRINT "ADRESS=";W + R;" READ=";E;" NOT ";R
1010 RETURN
