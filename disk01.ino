void printSector()
{
  int sixteen = 0;
  Serial.println("");
  for (byte q = 0; q < sectorSize; q++)
  {
    if (sector[q] < 0x10)
    {
      Serial.print("0");
    }

    Serial.print(sector[q], HEX);
    Serial.print(" ");
    sixteen++;

    if (sixteen == 16)
    {
      sixteen = 0;
      Serial.println("");
    }
  }
}

void HOME()
{
  Serial.println("");
  Serial.println("Going home.");
  Serial.print("data8:");
  Serial.print(data8, HEX);
  Serial.print("h / ");
  Serial.println(data8);
  curSector = 0;
  curTrack = 0;
}
